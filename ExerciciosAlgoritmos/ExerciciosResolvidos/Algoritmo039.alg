algoritmo "Algoritmo039"
// Função : Mostrar na tela a contagem de 10 até 3
// Autor : Lucas Mendes
// Data : 08/01/2024
// Seção de Declarações
   // Cont: Contador
var
   Cont: Inteiro
inicio
      Cont <- 10
      Enquanto (Cont >= 3) faca
               Escreva(Cont)
               Cont <- Cont - 1
      FimEnquanto
      Escreva(" Acabou!")
fimalgoritmo
