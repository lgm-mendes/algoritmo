algoritmo "Algoritmo038"
// Função : Mostrar na tela a contagem de 6 até 11
// Autor : Lucas Mendes
// Data : 08/01/2024
// Seção de Declarações 
   // Cont: Contador
var
   Cont: Inteiro
inicio
      Cont <- 6
      Enquanto (Cont <= 11) faca
               Escreva(Cont)
               Cont <- Cont + 1
      FimEnquanto
      Escreva(" Acabou!")
fimalgoritmo
